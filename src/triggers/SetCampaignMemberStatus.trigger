trigger SetCampaignMemberStatus on Campaign (after insert) {
	//change default member statuses (Sent and Responded) for select campaigns

	Set <Id> cmpns1 = new Set <Id>();
	Set <Id> cmpns2 = new Set <Id>();

	RecordType rt1 = [Select Id, Name from RecordType where Name = 'Program Campaign' limit 1];
	RecordType rt2 = [Select Id, Name from RecordType where Name = 'Conference/Event Campaign' limit 1];
	RecordType rt3 = [Select Id, Name from RecordType where Name = 'Childcare' limit 1];

	for (Campaign c: trigger.new){ 
		if (c.RecordTypeId == rt1.Id || c.RecordTypeId == rt2.Id) {	cmpns1.add(c.Id);}
		if (c.RecordTypeId == rt3.Id) {	cmpns2.add(c.Id);}
	}

	List<CampaignMemberStatus> cms2Delete = new List<CampaignMemberStatus>();
	List<CampaignMemberStatus> cms2Insert = new List<CampaignMemberStatus>();

	for (CampaignMemberStatus cm: [Select Id, Label, CampaignID  FROM CampaignMemberStatus WHERE CampaignID IN :cmpns1]){
  		if(cm.Label == 'Sent'){	        

    		CampaignMemberStatus cms2 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Family Attendee', HasResponded=true, IsDefault = True, SortOrder=3);
    		cms2Insert.add(cms2);
  			cms2Delete.add(cm);
 		}
		else if(cm.Label == 'Responded' ){

			CampaignMemberStatus cms1 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Professional Attendee', HasResponded=true, IsDefault = false, SortOrder=7);
  			CampaignMemberStatus cms2 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Child Attendee', HasResponded=true, IsDefault = false, SortOrder=8);
  			CampaignMemberStatus cms3 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Presenter', HasResponded=true, IsDefault = false, SortOrder=9);	  
  			CampaignMemberStatus cms4 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Interpreter', HasResponded=true, IsDefault = false, SortOrder=10);
  			CampaignMemberStatus cms5 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Exhibitor', HasResponded=true, IsDefault = false, SortOrder=11);
  			CampaignMemberStatus cms6 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Facilitator', HasResponded=true, IsDefault = false, SortOrder=12);
  			CampaignMemberStatus cms7 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Waitlist Adult', HasResponded=true, IsDefault = false, SortOrder=13);
  			CampaignMemberStatus cms8 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Waitlist Child', HasResponded=true, IsDefault = false, SortOrder=14);	   
  			
  			cms2Insert.add(cms1);
  			cms2Insert.add(cms2);
  			cms2Insert.add(cms3);
  			cms2Insert.add(cms4);
  			cms2Insert.add(cms5);
  			cms2Insert.add(cms6);
  			cms2Insert.add(cms7);
  			cms2Insert.add(cms8);
  			cms2Delete.add(cm);
 		}
	}
	
	for (CampaignMemberStatus cm: [Select Id, Label, CampaignID  FROM CampaignMemberStatus WHERE CampaignID IN :cmpns2]){
  		if(cm.Label == 'Sent'){	        

    		CampaignMemberStatus cms2 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Caregiver', HasResponded=true, IsDefault = true, SortOrder=3);
    		cms2Insert.add(cms2);
  			cms2Delete.add(cm);
 		}
		else if(cm.Label == 'Responded' ){

			CampaignMemberStatus cms1 = new CampaignMemberStatus(CampaignId=cm.CampaignID, Label='Child', HasResponded=true, IsDefault = false, SortOrder=7);
  			cms2Insert.add(cms1);
  			cms2Delete.add(cm);
 		}
	}
	
	//perform insert before delete because system requires at least one CMS for a Campaign
	insert cms2Insert;
	delete cms2Delete; 
	
