<apex:page readOnly="true" controller="Dynamic_Report_ByAge" 
           docType="html-5.0" sidebar="false" >
    
    <style>
        .centerHeader { text-align:center;}
    </style>
    <style>
        .centerHeaderBold { text-align:center; font-weight:bold; }
    </style>
    
    <apex:form >
        
        <apex:pageBlock title="AnnualReport Dashboard">
            <apex:pageMessages id="messages" ></apex:pageMessages>
            <apex:pageBlockSection title="Enter Date Range" columns="1" collapsible="false">
                <apex:panelGroup >
                    <apex:outputLabel value="Date From" />
                    <apex:input label="Date From" value="{!dateFrom}" type="auto" /> 
                    <apex:outputLabel value="Date To" />
                    <apex:input label="Date To" value="{!dateTo}" type="auto" />
                    <apex:outputLabel value="" />
                    <apex:commandButton value="Generate" action="{!startGenerateBoard}"  />
                    
                    
                </apex:panelGroup>          
            </apex:pageBlockSection> 
            
            <apex:pageblock title="Age Ranges  - Unduplicated" >
                <apex:pageBlockSection columns="3">
                    
                    <apex:pageBlockTable value="{!GenerateAge1}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue="3 - 5 Months "/>
                    </apex:pageBlockTable>
                    
                    <apex:pageBlockTable value="{!GenerateAge2}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue=" 6 - 11 Months "/>
                    </apex:pageBlockTable>
                    
                    <apex:pageBlockTable value="{!GenerateAge3}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue="12 - 17 Months"/>
                    </apex:pageBlockTable>
                    
                    
                    <apex:pageBlockTable value="{!GenerateAge4}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue="18 - 23 Months"/>
                    </apex:pageBlockTable>
                    
                    <apex:pageBlockTable value="{!GenerateAge5}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue="24 - 29 Months"/>
                    </apex:pageBlockTable>
                    
                    <apex:pageBlockTable value="{!GenerateAge6}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue="30 - 35 Months"/> -
                    </apex:pageBlockTable>
                    
                    <apex:pageBlockTable value="{!GenerateAge7}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue="3 to 5 Yrs"/> -
                    </apex:pageBlockTable>
                    
                    <apex:pageBlockTable value="{!GenerateAge8}" var="a">  
                        <apex:column value="{!a.ag1}" headervalue=" Gender" />
                        <apex:column value="{!a.c1}" headervalue=" Unknown Age"/> -
                    </apex:pageBlockTable>
         
                </apex:pageBlockSection>  
            </apex:pageblock>
            
        </apex:pageBlock>
    </apex:form>
</apex:page>